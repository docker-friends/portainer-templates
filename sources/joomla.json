{
  "type": 1,
  "title": "Joomla",
  "description": "Another free and open-source CMS",
  "categories": [
    "Cms"
  ],
  "platform": "linux",
  "logo": "https://djeqr6to3dedg.cloudfront.net/repo-logos/library/joomla/live/logo-1720462215828.png",
  "image": "joomla:latest",
  "env": [
    {
      "name": "JOOMLA_DB_HOST",
      "label": "MySQL database host",
      "type": "container"
    },
    {
      "name": "JOOMLA_DB_PASSWORD",
      "label": "Database password"
    }
  ],
  "name": "joomla",
  "ports": [
    "80/tcp"
  ],
  "volumes": [
    {
      "container": "/var/www/html"
    }
  ],
  "maintainer": "https://github.com/joomla-docker/docker-joomla"
}